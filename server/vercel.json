{
    "version": 2,
    "crons": [
        {
            "path": "/imports/cron-trigger",
            "schedule": "0 * * * *"
        }
    ],
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts"
        }
    ]
}